{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(png|jpg)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "images-api",
      "urls": [
        "https://picsum.photos/v2/list"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "1h",
        "timeout": "1s",
        "strategy": "freshness"
      }
    }
  ]
}
